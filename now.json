{
  "name": "prlint",
  "version": 2,
  "alias": "prlint-staging",
  "env": {
    "PRIVATE_KEY": "@prlint-private-key",
    "WEBHOOK_SECRET": "@prlint-webhook-secret",
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "handler.js"
    }
  ],
  "regions": [
    "all"
  ]
}